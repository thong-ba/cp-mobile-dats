{
  "expo": {
    "name": "MobDoAn",
    "slug": "MobDoAn",
    "owner": "thonglnses-organization",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mobdoan",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/icon.png",
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon.png"
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFE600"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.thonglnsesorganization.MobDoAn"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "reactCompiler": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "eas": {
        "projectId": "fbdd5cf6-e4fd-4dfd-9ff0-1e8ad45796e7"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/fbdd5cf6-e4fd-4dfd-9ff0-1e8ad45796e7"
    }
  }
}
